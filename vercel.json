{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/webpack",
      "config": {
        "entrypoints": ["./index.web.js"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(assets/.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
